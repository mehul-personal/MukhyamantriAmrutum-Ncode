<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ncode.mukhyamantriamrutum">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".ApplicationData"
        android:allowBackup="true"
        android:icon="@drawable/ic_app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashScreenActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NavigationDrawerActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DialogActivity"
            android:label="@string/app_name"
            android:theme="@style/CustomTransparentPage" />
        <activity
            android:name=".SearchDialogActivity"
            android:label="@string/app_name"
            android:theme="@style/CustomTransparentPage" />
        <activity
            android:name=".MapsActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".SearchEnrollementKisokActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".SearchEmpanelledHospitalActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".MyAccountActivity"
            android:label="@string/app_name" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_app_id" />

    </application>

</manifest>